{
  "name": "snapple-facts",
  "version": "2.2.0",
  "description": "Every Snapple Fact",
  "main": "./src/index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts --ext .js",
    "format": "prettier --write . --ext .ts --ext .js",
